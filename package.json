{
  "name": "rapid-launch-agent",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "node server/index.js",
    "migrate": "node server/migrate.js",
    "deploy": "node server/migrate.js && node server/index.js",
    "type-check": "tsc --noEmit",
    "test:api": "node test-api.js",
    "test": "npm run type-check && npm run build",
    "docker:build": "docker build -t rapid-launch-agent .",
    "docker:run": "docker run -p 5000:5000 --env-file .env rapid-launch-agent",
    "validate": "node -c server/index.js && npm run type-check"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@clerk/clerk-react": "^5.53.3",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "axios": "^1.6.2",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
